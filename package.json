{
  "name": "nextech",
  "version": "0.0.1",
  "description": "Nextech - Sistema com Angular + Electron",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "npm run build && electron .",
    "dev:electron": "concurrently -k \"ng serve\" \"wait-on http://localhost:4200 && electron .\"",
    "build": "ng build --output-path=dist/nextech-frontend-main --base-href ./",
    "build:ssr": "ng build && ng run Nextech-frontend-main:server:production",
    "serve:ssr": "node dist/server/main.js",
    "electron": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "build:electron": "npm run build && electron-builder",
    "make:win": "npm run build && electron-builder --win --x64"
  },
  "build": {
    "appId": "com.nextech.app",
    "productName": "Nextech",
    "files": [
      "dist/nextech-frontend-main/browser/",
      "main.js",
      "preload.js",
      "package.json"
    ],
    "directories": {
      "buildResources": "build"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/favicon.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "publish": [
      {
        "provider": "github",
        "owner": "A-Machado28",
        "repo": "Nextech_Frontend"
      }
    ]
  },
  "author": "Juvenal Caterca",
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.11",
    "@angular/cdk": "^19.2.9",
    "@angular/common": "^19.2.11",
    "@angular/compiler": "^19.2.11",
    "@angular/core": "^19.2.11",
    "@angular/forms": "^19.2.11",
    "@angular/platform-browser": "^19.2.11",
    "@angular/platform-browser-dynamic": "^19.2.11",
    "@angular/platform-server": "^19.2.11",
    "@angular/router": "^19.2.11",
    "@angular/service-worker": "^19.2.11",
    "@angular/ssr": "^19.2.5",
    "@nguniversal/express-engine": "^7.0.2",
    "@ngx-loading-bar/core": "^7.0.0",
    "@ngx-loading-bar/http-client": "^7.0.0",
    "@popperjs/core": "^2.11.8",
    "@primeng/themes": "^19.0.10",
    "body-parser": "^2.2.0",
    "bootstrap": "^5.3.6",
    "bootstrap-icons": "^1.13.1",
    "chart.js": "^4.4.9",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "electron-updater": "^6.6.2",
    "express": "^4.21.2",
    "html2canvas": "^1.4.1",
    "json": "^11.0.0",
    "json-server": "^1.0.0-beta.3",
    "jspdf": "^3.0.1",
    "jwt-decode": "^4.0.0",
    "mysql2": "^3.14.0",
    "ng2-charts": "^4.1.1",
    "ngx-bootstrap": "^19.0.2",
    "ngx-mask": "^19.0.7",
    "ngx-toastr": "^19.0.0",
    "npm": "^10.9.2",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primeng": "^19.1.3",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.7.3",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^1.17.1",
    "@angular-devkit/build-angular": "^19.2.5",
    "@angular/cli": "^19.2.5",
    "@angular/compiler-cli": "^19.2.0",
    "@types/bootstrap": "^5.2.10",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "concurrently": "^9.1.2",
    "electron": "^36.5.0",
    "electron-builder": "^26.0.12",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.7.2",
    "vite": "^6.3.5",
    "wait-on": "^8.0.3"
  }
}
